{
    "compilerOptions": {
        "target": "es2016",
        "lib": ["es6"],
        "module": "ES6",
        "resolveJsonModule": true,
        "allowJs": true,
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "moduleResolution": "Node"
    }
}
